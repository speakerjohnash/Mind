{
	"mode": "train",
	"dataset": "data/thoughts.csv",
	"label_map": {"0": "Predict", "1": "State", "2": "Ask", "3": "Reflect"},
	"label_key": "Type",
	"alphabet": "abcdefghijklmnopqrstuvwxyz0123456789,;.!?:'\"/\\|_@#$%^&*~`+-=<>()[]{}",
	"batch_size": 128,
	"doc_length": 256,
	"randomize": 5e-2,
	"momentum": 0.9,
	"base_rate": 0.04,
	"decay": 1e-5,
	"epochs": 5000,
	"eras": 25,
	"se_dim": 8
}
